{
  "coverageAnalysis": "perTest",
  "tsconfigFile": "tsconfig.stryker.json",
  "mochaOptions": {
    "spec": [
      "dist/test/helpers/**/*.js",
      "dist/test/unit/**/*.js"
    ]
  },
  "testFramework": "mocha",
  "testRunner": "mocha",
  "reporters": [
    "progress",
    "html",
    "dashboard"
  ],
  "maxConcurrentTestRunners": 4,
  "dashboard": {
    "reportType": "full"
  }
}