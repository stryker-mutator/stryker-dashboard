    "@types/chai": "4.3.16",
    "@types/chai-as-promised": "7.1.8",
    "@types/mocha": "10.0.7",
    "@types/node": "20.14.10",
    "@types/sinon": "17.0.3",
    "azurite": "3.31.0",
    "c8": "10.1.2",
    "chai": "5.1.1",
    "chai-as-promised": "8.0.0",
    "concurrently": "8.2.2",
    "lerna": "8.1.6",
    "mocha": "10.6.0",
    "prettier": "3.3.2",
    "prettier-plugin-tailwindcss": "0.6.5",
    "rimraf": "6.0.1",
